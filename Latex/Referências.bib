@book{NicholasJ.Gotelli;AaronM.Ellison2010,
abstract = {A comprehensive, but simple-to-use software package for executing a range of standard numerical analysis and operations used in quantitative paleontology has been developed. The program, called PAST (PAleontological STatistics), runs on stan- dard Windows computers and is available free of charge. PAST integrates spread- sheet-type data entry with univariate and multivariate statistics, curve fitting, time- series analysis, data plotting, and simple phylogenetic analysis. Many of the functions are specific to paleontology and ecology, and these functions are not found in stan- dard, more extensive, statistical packages. PAST also includes fourteen case studies (data files and exercises) illustrating use of the program for paleontological problems, making it a complete educational package for courses in quantitative methods.},
author = {{Nicholas J. Gotelli; Aaron M. Ellison}},
booktitle = {System},
isbn = {9788536324326},
title = {{Princ{\'{i}}pios de Estat{\'{i}}stica em Ecologia}},
year = {2010}
}
