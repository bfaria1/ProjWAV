author = {{Dias da Silva}, Jos{\'{e}} Ant{\^{o}}nio and Nery, Aline Silva Dejosi},
doi = {10.15536/thema.v16.2019.607-616.1344},
file = {:C$\backslash$:/Users/bia99/OneDrive/Documents/Wikiaves/Considera{\c{c}}{\~{o}}es/1344-6864-1-PB.pdf:pdf},
